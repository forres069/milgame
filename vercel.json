{
    "version": 2,
    "builds": [{
        "src": "milgame/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.10"
        }
      }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "milgame/wsgi.py"
      }
    ]
}
  
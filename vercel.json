{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    },
    {
      "src": "milgame/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambda size": "15mb", "runtime": "python3.10.10"}
    }
  ],
  "routes": [
    {
      "src": "/((?!general).*)",
      "dest": "milgame/wsgi.py"
    },
    {
      "src": "/((?!general).*)",
      "dest": "frontend/"
    }
  ]
}

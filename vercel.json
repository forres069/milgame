{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    },
    {
      "src": "milgame/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "milgame/wsgi.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "HEAD", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/",
      "methods": ["GET", "HEAD"]
    }
  ]
}

{
    "version": 2,
    "builds": [
      {
        "src": "frontend",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"
        }
      },
      {
        "src": "milgame/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.10.10"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "milgame/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build"
      }
    ]
  }
{
    "version": 2,
    "builds": [
        {
        "src": "milgame/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.10"
            }
        },
        {
            "src": "frontend/build",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "frontend/build"
            }
        }
    ],
        "routes": [
            {
                "src": "frontend/(.*)",
                "dest": "frontend/build/$1"
            },
            {
                "src": "/(.*)",
                "dest": "milgame/wsgi.py"
            }
        ]
}
  